<?xml version="1.0"?>
<Container version="2">
  <Name>HomePage</Name>
  <Repository>ghcr.io/gethomepage/homepage:latest</Repository>
  <Registry>https://github.com/gethomepage/homepage</Registry>
  <Network>bridge</Network>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/gethomepage/homepage/issues</Support>
  <Project>https://github.com/gethomepage/homepage</Project>
  <Overview>A modern, fully static, fast, secure fully proxied, highly customizable application dashboard with integrations for over 100 services and translations into multiple languages. Easily configured via YAML files or through docker label discovery.</Overview>
  <Category>HomeAutomation: Productivity: Tools: Other:</Category>
  <WebUI>http://[IP]:[PORT:3000]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/xshatterx/Seraphys/refs/heads/master/HomePage.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <Config Name="WebUI" Target="3000" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="true" Mask="false"/>
  <Config Name="Config" Target="/app/config" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Allowed Hosts" Target="HOMEPAGE_ALLOWED_HOSTS" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="false">IPADDRESS:PORT</Config>
  <Config Name="Docker Socket" Target="/var/run/docker.sock" Default="" Mode="ro" Description="" Type="Path" Display="always" Required="true" Mask="false">/var/run/docker.sock</Config>
</Container>